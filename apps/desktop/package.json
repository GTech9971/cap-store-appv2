{
  "name": "cap-store-app",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "icon": "tauri icon src-tauri/icon/画像.png",
    "build": "tsc && vite build",
    "bundle:dev": "tauri build --debug --config ./src-tauri/tauri.conf.dev.json && tauri bundle --bundles dmg",
    "bundle:stg": "tauri build --debug --config ./src-tauri/tauri.conf.stg.json && tauri bundle --bundles dmg",
    "bundle:prod": "tauri build --debug --config ./src-tauri/tauri.conf.prod.json && tauri bundle --bundles dmg",
    "bundle:prod:windows": "tauri build --debug --config ./src-tauri/tauri.conf.prod.windows.json && tauri bundle",
    "preview": "vite preview",
    "tauri": "tauri",
    "lint": "eslint ."
  },
  "dependencies": {
    "@ionic/core": "^8.7.5",
    "@ionic/react": "^8.7.5",
    "@okta/okta-react": "^6.10.0",
    "@tauri-apps/api": "^2.8.0",
    "@tauri-apps/plugin-clipboard-manager": "^2.3.0",
    "@tauri-apps/plugin-dialog": "^2.4.0",
    "@tauri-apps/plugin-opener": "^2",
    "axios": "^1.12.2",
    "cap-store-api-def": "^1.0.13",
    "ionicons": "^8.0.13",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.9.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@tauri-apps/cli": "^2.8.4",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "msw": "^2.11.3",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.24.1",
    "vite": "^7.0.4"
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  }
}
